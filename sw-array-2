let numeri=[3,5,10,2,8];

let somma= numeri.reduce((acc,n)=> acc+n);
console.log(somma);

let media= somma / numeri.length
console.log(media);

let ValoriMinoriMedia= numeri.filter(numeri=>numeri<media)
console.log(ValoriMinoriMedia);

let ValoriMaggioriMedia= numeri.filter(numeri=>numeri>media)
console.log(ValoriMaggioriMedia);
