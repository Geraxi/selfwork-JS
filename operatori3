let num1= parseInt(prompt('inserisci il primo numero'));
let num2= parseInt(prompt('inserisci il secondo numero'));
let somma= num1+num2;
let sottrazione= num1-num2;
let moltiplicazione= num1*num2;
let divisione= num1/num2;
let potenza= num1**num2;

console.log('con i numeri da te scelti, i risultati delle varie operazioni sono:');
console.log(`somma= ${somma}, sottrazione ${sottrazione}, moltiplicazione ${moltiplicazione}, divisone ${divisione}, potenza ${potenza}`);
